{
  "name": "coros-connect",
  "version": "0.1.3",
  "description": "Interface to connect with Coros API",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": "./dist/index.mjs"
    },
    "./dist/*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    }
  },
  "scripts": {
    "build": "find ./dist -type d -empty -delete; tsc && tsup",
    "example": "tsx example/index.ts",
    "format:check": "biome format src",
    "format:fix": "biome format --write src",
    "prepare": "npm run build"
  },
  "keywords": [
    "coros",
    "typescript",
    "running"
  ],
  "author": "Jose Miguel <jmn8718@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/jmn8718/coros-connect#readme",
  "bugs": {
    "url": "https://github.com/jmn8718/coros-connect/issues"
  },
  "files": [
    "dist/*",
    "examples/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmn8718/coros-connect.git"
  },
  "publishConfig": {
    "@jmn8718:registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "22.10.0",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "5.7.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.750.0",
    "app-root-path": "3.1.0",
    "axios": "1.7.9",
    "dayjs": "1.11.13",
    "jszip": "3.10.1",
    "ky": "1.7.2"
  }
}
